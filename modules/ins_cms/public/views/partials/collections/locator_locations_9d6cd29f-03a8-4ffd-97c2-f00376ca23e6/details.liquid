---
metadata: {
    slug: "{{ context.exports.data.item.slug }}",
    layout: "details",
    page_slug: "locator-locations",
    created_by: "QA Console",
    meta_title: "Website | {{ context.exports.data.item.location_name }} aa",
    page_title: "Website | {{ context.exports.data.item.location_name }}",
    canonical_url: "https://iia-uat.staging.oregon.platform-os.com/locator-locations{{ context.exports.data.item.slug }}",
    sitemap_order: "8",
    open_graph_url: "https://iia-uat.staging.oregon.platform-os.com/locator-locations{{ context.exports.data.item.slug }}",
    schema_content: "<script type=\"application/ld+json\">
	{
      	\"@context\": \"http://schema.org\",
        \"@type\": \"Brand\",
        \"logo\": \"https://cdn.prod.website-files.com/5d074f688518921b7700284e/64d5a764100b80bd164f88aa_ins-logo-black.svg\",
        \"description\": \"{{ context.exports.data.id }}\"
    }
</script>",
    last_updated_by: "QA Insites",
    open_graph_type: "website",
    meta_description: "Discover in-depth information about {{ context.exports.data.item.location_name }}, including its history, attractions, and visitor tips. Get all the details you need to make the most of your visit to this unique destination. aa",
    open_graph_image: "{{ context.exports.data.id }}",
    open_graph_title: "Website | {{ context.exports.data.item.location_name }} aa",
    sitemap_priority: ".8",
    is_image_database: "true",
    is_sitemap_enabled: "true",
    open_graph_site_name: "Website | {{ context.exports.data.item.location_name }}",
    open_graph_description: "Discover in-depth information about {{ context.exports.data.item.location_name }}, including its history, attractions, and visitor tips. Get all the details you need to make the most of your visit to this unique destination. aa",
    sitemap_change_frequency: "weekly",
    is_visible_to_search_engines: "true",
}
---

<div class="max-w-6xl mx-auto bg-white shadow-lg rounded-lg overflow-hidden">
        <div class="overflow-x-auto">
            <table class="w-full text-left border-collapse">
                <thead>
                    <tr class="bg-gray-100 border-b border-gray-200">
                        <th class="px-6 py-4 font-semibold text-gray-700 uppercase text-xs tracking-wider w-1/3">Field
                            Key</th>
                        <th class="px-6 py-4 font-semibold text-gray-700 uppercase text-xs tracking-wider">Liquid
                            Reference</th>
                    </tr>
                </thead>
                <tbody class="divide-y divide-gray-200">
                    <!-- Basic Information -->
                    <tr class="bg-gray-50">
                        <td colspan="2" class="px-6 py-2 font-bold text-indigo-600 text-sm italic">System Fields
                        </td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">ID</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.id }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">UUID</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.uuid }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Created At</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.created_at }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Updated At</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.updated_at }}</td>
                    </tr>

                    <!-- Details Tab -->
                    <tr class="bg-gray-50">
                        <td colspan="2" class="px-6 py-2 font-bold text-indigo-600 text-sm italic">Details Tab</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Location Name</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.location_name }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Location Slug</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.slug }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Category UUID</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.category_uuid }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Start Date</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.start_date }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">End Date</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.end_date }}</td>
                    </tr>

                    <!-- Details Tab -->
                    <tr class="bg-gray-50">
                        <td colspan="2" class="px-6 py-2 font-bold text-indigo-600 text-sm italic">Details Tab</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Short Description</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.short_description }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Long Description</td>
                        <td class="px-6 py-3 text-sm liquid-tag">{{ context.exports.data.item.long_description }}</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Location Image</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.location_image.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Pin Icon</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.pin_icon.url }}" alt=""></td>
                    </tr>

                    <!-- Media Tab -->
                    <tr class="bg-gray-50">
                        <td colspan="2" class="px-6 py-2 font-bold text-indigo-600 text-sm italic">Media Tab</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Image 1</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.image_1.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Image 2</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.image_2.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Image 3</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.image_3.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Image 4</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.image_4.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Image 5</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><img src="{{ context.exports.data.item.image_5.url }}" alt=""></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">File 1</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><a href="{{ context.exports.data.item.file_1.url }}">{{ context.exports.data.item.file_1.file_name }}</a></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">File 2</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><a href="{{ context.exports.data.item.file_2.url }}">{{ context.exports.data.item.file_2.file_name }}</a></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">File 3</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><a href="{{ context.exports.data.item.file_3.url }}">{{ context.exports.data.item.file_3.file_name }}</a></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">File 4</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><a href="{{ context.exports.data.item.file_4.url }}">{{ context.exports.data.item.file_4.file_name }}</a></td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">File 5</td>
                        <td class="px-6 py-3 text-sm liquid-tag"><a href="{{ context.exports.data.item.file_5.url }}">{{ context.exports.data.item.file_5.file_name }}</a></td>
                    </tr>

                    <!-- Gallery Tab -->
                    <tr class="bg-gray-50">
                        <td colspan="2" class="px-6 py-2 font-bold text-indigo-600 text-sm italic">Gallery Tab</td>
                    </tr>
                    <tr>
                        <td class="px-6 py-3 text-sm text-gray-600 font-medium">Gallery 1</td>
                        <td class="px-6 py-3 text-sm liquid-tag">
                            {% if context.exports.data.item.gallery_1.size > 0 %}
                                <div class="gallery-container">
                                    {% for item in context.exports.data.item.gallery_1 %}
                                        <a href="{{ item.id }}">{{ item.id }}</a>
                                    {% endfor %}
                                </div>
                            {% else %}
                                <p>No images found in the gallery.</p>
                            {% endif %}
                        </td>
                    </tr>

                </tbody>
            </table>
        </div>
    </div>